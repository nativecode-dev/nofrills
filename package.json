{
  "name": "nofrills",
  "private": true,
  "version": "1.0.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "@types/chai": "^4.1.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "*",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "cz-lerna-changelog": "^1.2.1",
    "lerna": "^2.11.0",
    "lerna-semantic-release": "^9.1.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-no-unused-expression-chai": "^0.1.3",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "node_modules",
      "packages/*/src/**/*.spec.ts",
      "packages/*/src/Logger.ts",
      "packages/*/src/Logging.ts",
      "packages/*/src/cli-*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "core/*/src/**/*.ts",
      "packages/*/src/**/*.ts",
      "packages-cli/*/src/**/*.ts",
      "packages-typings/*/src/**/*.ts"
    ],
    "instrument": true,
    "report-dir": ".cache/coverage",
    "reporter": "text",
    "sourceMap": true,
    "temp-directory": ".cache/nyc"
  },
  "scripts": {
    "build": "cli-tasks build",
    "clean": "cli-tasks clean",
    "commit": "cli-tasks commit",
    "coveralls": "cli-tasks coverage",
    "release": "cli-tasks release",
    "start": "cli-tasks start",
    "test": "cli-tasks test",
    "ssh": "set-up-ssh --key \"$encrypted_fa989ae4fdf1_key\" --iv \"$encrypted_fa989ae4fdf1_iv\" --path-encrypted-key \"deploy.enc\""
  },
  "workspaces": {
    "packages": [
      "core/*",
      "packages/*",
      "packages-cli/*",
      "packages-typings/*"
    ]
  }
}
