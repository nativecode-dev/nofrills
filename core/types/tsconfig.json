{
  "extends": "../../tsconfig.settings",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "lib",
    "rootDir": "src",
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "paths": {
      "zipcodes-regex": [
        "types/zipcodes-regex"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "specs"
  ],
  "include": [
    "src/**/*.ts"
  ]
}
