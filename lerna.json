{
  "command": {
    "exec": {
      "bail": true
    },
    "publish": {
      "allowBranch": "master",
      "ignore": [
        "*.md",
        "LICENSE"
      ],
      "skipGit": false,
      "yes": true
    },
    "run": {
      "stream": true
    }
  },
  "lerna": "2.11.0",
  "loglevel": "error",
  "npmClient": "yarn",
  "npmClientArgs": [
    "--quiet"
  ],
  "packages": [
    "packages/*"
  ],
  "sort": true,
  "useWorkspaces": true,
  "version": "3.1.6-7"
}
