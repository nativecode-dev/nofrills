      "src/**/*.ts"
    ],
    "instrument": true,
    "report-dir": ".cache/coverage",
    "reporter": "text",
    "sourceMap": true,
    "temp-directory": ".cache/nyc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nativecode-dev/nofrills-types.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "prebuild": "yarn run clean",
    "clean": "rimraf .cache && rimraf coverage && rimraf lib"
  }
}
