{
  "author": "support@nativecode.com",
  "description": "Set of useful (maybe) collection types.",
  "homepage": "https://github.com/nativecode-dev/nofrills-collections#readme",
  "license": "MIT",
  "main": "./lib/index.js",
  "name": "@nofrills/collections",
  "types": "./lib/index.d.ts",
  "version": "0.10.1",
  "bugs": {
    "url": "https://github.com/nativecode-dev/nofrills-collections/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.16",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "tests/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "instrument": true,
    "sourceMap": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativecode-dev/nofrills-collections.git"
  },
  "scripts": {
    "build": "tsc --project .",
    "prebuild": "rimraf lib/",
    "postbuild": "yarn run lint",
    "lint": "tslint src/",
    "start": "node lib/",
    "prestart": "yarn run build",
    "test": "cross-env DEBUG= nyc mocha --opts mocha.opts",
    "pretest": "yarn run build",
    "testonly": "cross-env DEBUG= nyc mocha --opts mocha.opts",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "push": "npm publish --access public",
    "prepush": "yarn run test",
    "clean": "rimraf .nyc_output && rimraf coverage && rimraf lib"
  }
}
