{
  "author": "support@nativecode.com",
  "description": "Scrubs an object of all sensitive data.",
  "homepage": "https://github.com/nativecode-dev/nofrills-scrubs#readme",
  "license": "MIT",
  "main": "./lib/index.js",
  "name": "@nofrills/scrubs",
  "types": "./lib/index.d.ts",
  "version": "0.9.1",
  "bugs": {
    "url": "https://github.com/nativecode-dev/nofrills-scrubs/issues"
  },
  "dependencies": {
    "@nofrills/lincoln-debug": "^0.3.2",
    "@nofrills/types": "^0.9.0",
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/lodash": "^4.14.71",
    "@types/lodash.merge": "^4.6.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.10",
    "chai": "^4.0.2",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "rimraf": "^2.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "instrument": true,
		"report-dir": ".cache/coverage",
		"reporter": "text",
		"sourceMap": true,
		"temp-directory": ".cache/nyc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativecode-dev/nofrills-scrubs"
  },
  "scripts": {
    "build": "tsc --project .",
    "prebuild": "rimraf lib/",
    "postbuild": "yarn run lint",
    "lint": "tslint src/",
    "test": "cross-env DEBUG= nyc mocha --opts mocha.opts",
    "pretest": "yarn run build",
    "testonly": "cross-env DEBUG= nyc mocha --opts mocha.opts",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "push": "npm publish --access public",
    "prepush": "yarn run test",
    "clean": "rimraf .cache && rimraf coverage && rimraf lib"
  }
}
